    "prettier": "3.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-test-renderer": "18.2.0",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "typescript": "5.1.6",
    "vite": "4.4.2",
    "vite-plugin-linter": "2.0.2",
